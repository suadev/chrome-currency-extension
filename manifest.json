{
    "manifest_version": 2,
    "name": "Exchange Rates",
    "short_name": "Exchange Rates",
    "description": "Replace the new tab with a list of currecy rates.",
    "version": "1.0",
    "permissions": [
      "tabs",
      "storage",
      "webNavigation"
    ],
    "icons": {
      "16": "assets/img/icon16.png",
      "48": "assets/img/icon48.png",
      "128": "assets/img/icon128.png"
    },
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "homepage_url": "http://github.com/suadev",
    "browser_action": {
      "default_title": "Exchange Rates",
      "default_icon": "assets/img/icon128.png" 
    }
  }
  